{
  "name": "n8n-nodes-agenta",
  "version": "0.1.1",
  "description": "Agenta prompt management nodes for n8n",
  "license": "MIT",
  "homepage": "https://github.com/agenta-ai/n8n-nodes-agenta",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agenta-ai/n8n-nodes-agenta.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp src/agenta.svg dist/ && cp src/nodes/Agenta/agenta.svg dist/nodes/Agenta/ || true",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "agenta",
    "prompt-management",
    "ai",
    "llm",
    "machine-learning"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AgentaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Agenta/Agenta.node.js"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-core": ">=1.0.0",
    "n8n-workflow": ">=1.0.0"
  },
  "author": {
    "name": "Agenta AI",
    "email": "support@agenta.ai"
  },
  "bugs": {
    "url": "https://github.com/agenta-ai/n8n-nodes-agenta/issues"
  },
  "dependencies": {
    "@types/lodash": "^4.17.20",
    "https-proxy-agent": "^7.0.6",
    "lodash": "^4.17.21"
  }
}
